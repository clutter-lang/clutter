(define-function hello-world
  (bind-lexical-variables (x "Hello"
                           y "World")
    (lambda () (print x) (print y))))

((fun hello-world))

(bind-lexical-functions (hello-world (lambda (x) (print "Hello") (print x)))
  (hello-world "Syko"))

(hello-world)

(define-variable hello (quote Hello))

(bind-lexical-variables (hello-var (lambda () (print "Call me any, any time.")))
  ((var hello-var)))

(print hello)
(print (quote World!))

(quit)